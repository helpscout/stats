    "@storybook/addon-links": "^4.1.9",
    "@storybook/addons": "^4.1.9",
    "@storybook/react": "^4.1.9",
    "@types/react": "16.7.18",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "lerna": "^3.10.7",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
